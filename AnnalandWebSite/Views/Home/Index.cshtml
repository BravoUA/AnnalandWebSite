    <script type="text/javascript" src="jquery-3.1.1.js"></script>

    <script>
        $(window).scroll(function () { // Когда страница прокручивается
            var scrTop = $(window).scrollTop();
            if(scrTop>$('#Head6').offset().top - $(window).height()  ) {
                numAnimate();
            }
        });
        function numAnimate () {

            var currentNumber = $('#num-1').text();
            $({numberValue: currentNumber}).animate({numberValue: 2011}, {
                duration: 2000,
                easing: 'linear',
                step: function() {
                    $('#num-1').text(Math.ceil(this.numberValue));
                }
            });

            var currentNumber3 = $('#num-3').text();
            $({numberValue: currentNumber3}).animate({numberValue: 120}, {
                duration: 2000,
                easing: 'linear',
                step: function() {
                    $('#num-3').text(Math.ceil(this.numberValue));
                }
            });
            var currentNumber4 = $('#num-4').text();
            $({numberValue: currentNumber4}).animate({numberValue: 8}, {
                duration: 2000,
                easing: 'linear',
                step: function() {
                    $('#num-4').text(Math.ceil(this.numberValue));
                }
            });

        };
